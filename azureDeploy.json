    "contentVersion": "1.0.0.0",
    "parameters": {
        "CustomerName": {
            "type": "string",
            "metadata": {
                "description": "Name of the Virtual Machine."
            }
        }
    },
    "variables": {
        "UniqueString": "[substring(uniqueString(subscription().id), 0, 8)]"
