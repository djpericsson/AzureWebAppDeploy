{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "CustomerName": {
            "type": "string",
            "defaultValue": "Addlevel DEV",
            "metadata": {
                "description": "The Customer name."
            },
            "minLength": 1
        }
    },
    "variables": {
        "UniqueString": "[substring(uniqueString(subscription().subscriptionId), 0, 8)]"
    },
    "resources": [
    ],
    "outputs": {
        "UniqueString": {
            "type": "string",
            "value": "[variables('UniqueString')]"
        }
    }
}